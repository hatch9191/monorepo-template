{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "es2021"],
    "target": "es2021",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
  }
}
